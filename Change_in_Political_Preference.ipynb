    "# Small-World Network of Friends Generating Function"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 15,
   "metadata": {},
   "outputs": [],
   "source": [
    "def small_world_friends(n,k,p,s):\n",
    "    F = nx.watts_strogatz_graph(n,k,p,s) #number of node, degree, prob of re-wire, seed\n",
    "    \n",
    "    #node-weights:\n",
    "    for i in range(n):\n",
    "        F.node[i]['weight'] = random.uniform(-0.9,0.9)\n",
    "    \n",
    "    #link weights:\n",
    "    for i in F:\n",
    "        for j in F:\n",
    "            if F.has_edge(i,j):\n",
    "                F[i][j]['weight'] = friW\n",
    "    \n",
    "    return F\n"
   ]
