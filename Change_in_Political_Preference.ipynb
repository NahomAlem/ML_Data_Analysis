   "source": [
    "# Categorizing nodes \n",
    "def categorize(z):\n",
    "    dem = []\n",
    "    rep = []\n",
    "    moderate = []\n",
    "    for n in z.nodes():\n",
    "        pref = z.node[n]['weight']\n",
    "        if  -0.1 <= pref <= 0.1:\n",
    "            moderate.append(n)\n",
    "        elif pref > 0:\n",
    "            rep.append(n)\n",
    "        else: \n",
    "            dem.append(n)\n",
    "    return dem, rep, moderate"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 7,
   "metadata": {},
   "outputs": [],
   "source": [
    "def coloredEdges(z):\n",
    "    friends = []\n",
    "    family = []\n",
    "    spousal = []\n",
    "    for i in z.nodes():\n",
    "        for j in z.neighbors(i):\n",
    "            connection = z[i][j]['weight']\n",
    "            if connection == 0.7: #family\n",
    "                family.append([i,j])\n",
    "            elif connection == 0.67: #friends\n",
    "                friends.append([i,j])\n",
    "            elif connection == 0.6: #spousal\n",
    "                spousal.append([i,j])\n",
    "                \n",
    "    return friends, family, spousal"
   ]
