   "source": [
    "#added\n",
    "#sum the weight of everything\n",
    "def summation(z):\n",
    "    totcon = 0\n",
    "    totlib = 0\n",
    "    totmod = 0\n",
    "\n",
    "    for n in z.nodes():\n",
    "        pref = z.node[n]['weight']\n",
    "        if -0.1 <= pref <= 0.1:\n",
    "            totmod += pref\n",
    "        elif pref > 0:\n",
    "            totcon += pref\n",
    "        else: \n",
    "            totlib += pref\n",
    "    return totcon, totlib, totmod"
   ]
  },
